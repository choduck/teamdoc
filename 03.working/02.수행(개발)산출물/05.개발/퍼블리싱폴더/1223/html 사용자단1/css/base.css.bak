@charset "utf-8";




body {margin:0px; padding:0px;  font-family:"나눔고딕", Dotum, "돋움", Arial, serif ; font-size:13px;  height:100%; background:url(../image/bg.gif) repeat; }
html{height:100%;}
div, dl, dt, dd, ul, ol, li, pre, form, fieldset, input, textarea, p, blockquote, th, td {margin:0px; padding:0px;}

header,footer,section,article,aside,nav,hgroup,details,menu,figure,figcaption{display:block;border:0;*margin:0;*padding:0;*font-size:0; padding:0px; margin:0px;}/* html5 */
